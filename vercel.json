{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/react"
      },
      {
        "src": "api/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/src/$1"
      }
    ]
  }
  